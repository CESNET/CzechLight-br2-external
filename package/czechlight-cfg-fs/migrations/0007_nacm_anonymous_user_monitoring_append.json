{
  "ietf-netconf-acm:nacm": {
    "rule-list": [
      {
        "name": "Permit yangnobody user/group to read only some modules",
        "group": [
          "yangnobody"
        ],
        "rule": [
          {
            "@": {
              "yang:insert": "last"
            },
            "name": "ietf-restconf-monitoring",
            "module-name": "ietf-restconf-monitoring",
            "action": "permit",
            "access-operations": "read"
          }
        ]
      }
    ]
  }
}
