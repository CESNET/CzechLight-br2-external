# this is a part of the Clearfog Base carrier
CONFIG_SENSORS_MCP3021=y

# PMBus for the PSUs
CONFIG_PMBUS=y
CONFIG_SENSORS_PMBUS=y

# SPI-to-Quad-UART
CONFIG_SERIAL_MAX310X=y

# mvneta
CONFIG_MVNETA_BM_ENABLE=y
CONFIG_MVPP2=n

# Russel King's SFP patches and some Clearfog-specific bits as well
CONFIG_LOCALVERSION="-cfb-sfp"
CONFIG_SFP=y
