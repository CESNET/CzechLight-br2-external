			label = "port3:red";
			reg = <5>;
		};
		p4_green@6 {
			label = "port4:green";
			reg = <6>;
		};
		p4_red@7 {
			label = "port4:red";
			reg = <7>;
		};
		p5_green@8 {
			label = "port5:green";
			reg = <8>;
