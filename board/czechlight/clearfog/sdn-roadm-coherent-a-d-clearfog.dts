#include "sdn-roadm-clearfog.dtsi"

&lumentum_ocm {
	status = "disabled";
};

&i2c1 {
	tlc59116@61 {
		#address-cells = <1>;
		#size-cells = <0>;
		#gpio-cells = <2>;
		compatible = "ti,tlc59116";
		reg = <0x61>;

		p1_green@0 {
			label = "port1:green";
			reg = <0>;
		};
		p1_red@1 {
			label = "port1:red";
			reg = <1>;
		};
		p2_green@2 {
			label = "port2:green";
			reg = <2>;
		};
		p2_red@3 {
			label = "port2:red";
			reg = <3>;
		};
