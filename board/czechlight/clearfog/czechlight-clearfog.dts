#include "armada-388-clearfog-base.dts"

&w25q32 {
	status = "okay";
};

/ {
	clocks {
		spi_uart_clk: osc_max14830 {
			compatible = "fixed-clock";
			#clock-cells = <0>;
			clock-frequency = <3686400>;
		};
	};

	onewire@0 {
		compatible = "w1-gpio";
		gpios = <&gpio0 25 GPIO_ACTIVE_HIGH>;
		pinctrl-0 = <&hack_pins>;
		pinctrl-names = "default";
	};
};

&uart1_pins {
        status = "disabled";
};

&uart1 {
        status = "disabled";
};

&pinctrl {
        hack_pins: hack_pins {
                marvell,pins = "mpp25";
                marvell,function = "gpio";
        };
};

&gpio0 {
	mikrobus_uart_tx {
		gpios = <25 GPIO_ACTIVE_HIGH>;
		line-name = "onewire-gpio";
	};
};
