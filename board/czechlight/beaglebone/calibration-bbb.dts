#include "czechlight-bbb.dts"

&i2c1 {
	gpio_exp_u3: gpio@0x26 {
		compatible = "microchip,mcp23017";
		reg = <0x26>;

		gpio-controller;
		#gpio-cells = <2>;

		gpio-line-names =
			"S0_LW3",
			"M2_LW3",
			"M0_LW3",
			"M1_LW3",
			"M3_LW3",
			"S1_LW3",
