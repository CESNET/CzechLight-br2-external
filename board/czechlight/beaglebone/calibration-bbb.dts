			"M3_LW2",
			"S1_LW2",
			"S0_LW2",
			"M2_LW2",
			"M0_LW2";
	};

	gpio_exp_u4: gpio@0x27 {
		compatible = "microchip,mcp23017";
		reg = <0x27>;

		gpio-controller;
		#gpio-cells = <2>;

		gpio-line-names =
